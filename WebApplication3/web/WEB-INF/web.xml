<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servletcontrollerApp</servlet-name>
        <servlet-class>controller.servletcontrollerApp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testlogin</servlet-name>
        <servlet-class>controller.testlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet</servlet-name>
        <servlet-class>controller.servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>table2dao</servlet-name>
        <servlet-class>model.table2dao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletcontrollerApp</servlet-name>
        <url-pattern>/servletcontrollerApp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testlogin</servlet-name>
        <url-pattern>/testlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>table2dao</servlet-name>
        <url-pattern>/table2dao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
